// JSON to javascript

const jsonData = '{ "name": "John", "age": 22 }';

console.log(jsonData);

// converting to JavaScript object

const obj = JSON.parse(jsonData);

// accessing the data

console.log(obj);

console.log('..........................');

// javascript to JSON

