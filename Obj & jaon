//Example 1

const foo = [1, 2, 3];

const bar = [...foo];

console.log(bar);

//Example 2

const ak = [1,'ik',8];

const bb = [...ak];

console.log(bb);

//Example 3

const all = [...bar,...bb];

console.log(all)

